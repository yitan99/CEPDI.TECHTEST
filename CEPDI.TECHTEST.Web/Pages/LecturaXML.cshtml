@page
@model LecturaXMLModel
@{
    ViewData["Title"] = "Lectura XML y Consumo Web Service";
}
<h1>@ViewData["Title"]</h1>
<p>Lectura XML y Consumo Web Service.</p>
<div class="form-group">
    <label for="DocumentUploadTB">Arrastrar  y colocar archivo para descargar archivo resultado PDF</label>
    <div class="form-control-static">
        <span class="btn btn-success fileinput-button">
            <span>Agregar Archivo...</span>
            <input type="file" id="DocumentUploadTB" />
        </span>
    </div>
    <button type="button" class="btn btn-primary" id="btnAdd" onclick="return EnviarXML();">Enviar XML</button>
</div>

<div class="modal fade" id="myModalX" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content" style="width:850px;height:900px;">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" onclick="cerrarModalX()">×</button>
                <h4 class="modal-title" id="myModalLabel">Archivo PDF</h4>
            </div>
            <div class="modal-body">
                <iframe id="filePDF" width='800px' height='850px' >
                    </iframe> 
            </div>
            <div class="modal-footer">
            </div>
        </div>
    </div>
</div>
